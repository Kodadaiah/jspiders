<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="hotSpot" class="com.om.spring.bean.HotSpot">
		<property name="operator" value="Airtel"></property>
		<property name="power" ref="tataPower"></property>
	</bean>

	<bean id="tataPower" class="com.om.spring.bean.Power">
		<property name="volts" value="12"></property>
	</bean>

</beans>